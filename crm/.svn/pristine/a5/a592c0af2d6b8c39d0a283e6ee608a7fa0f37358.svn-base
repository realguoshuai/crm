<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>zy_crm</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>setCharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>RoleAddServlet</display-name>
    <servlet-name>RoleAddServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.role.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleAddServlet</servlet-name>
    <url-pattern>/role/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RoleDeleteServlet</display-name>
    <servlet-name>RoleDeleteServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.role.DeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleDeleteServlet</servlet-name>
    <url-pattern>/role/delete</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RoleListServlet</display-name>
    <servlet-name>RoleListServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.role.RoleListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleListServlet</servlet-name>
    <url-pattern>/role/list</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RoleUpdateServlet</display-name>
    <servlet-name>RoleUpdateServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.role.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoleUpdateServlet</servlet-name>
    <url-pattern>/role/update</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DepartmentAddServlet</display-name>
    <servlet-name>DepartmentAddServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.department.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DepartmentAddServlet</servlet-name>
    <url-pattern>/department/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DepartmentListServlet</display-name>
    <servlet-name>DepartmentListServlet</servlet-name>
    <servlet-class>com.zhiyou100.servlet.department.ListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DepartmentListServlet</servlet-name>
    <url-pattern>/department/list</url-pattern>
  </servlet-mapping>
</web-app>